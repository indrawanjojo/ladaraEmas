			var mb = "<?php echo $encidu; ?>";
			$('#loading-ct').show();
			jQuery.ajax({
			url: "<?php echo $urlapi ?>" + "service/ct_member",
			cache: false,
			dataType: 'json',
			data: {mb: mb },
			success:function(output_string){
				$('#stat_ct').append("<span class='glyphicon glyphicon-credit-card' aria-hidden='true'></span>  Rp. " + currencyFormatDE(parseFloat(output_string)));
				$('#loading-ct').hide();
			},
			error: function(){
				$('#stat_ct').html('<b>Error</b>');
				$('#loading-ct').hide();
			 }
			});
		});
